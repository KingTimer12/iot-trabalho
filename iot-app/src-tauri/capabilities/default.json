{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "websocket:default",
    {
      "identifier": "http:default",
      "allow": [{ "url": "http://localhost:80" }]
    }
  ]
}